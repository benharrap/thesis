@article{murray2010,
author = {Murray, Lyn and Tarren-Sweeney, Michael and France, Karyn},
title = {Foster carer perceptions of support and training in the context of high burden of care},
journal = {Child \& Family Social Work},
volume = {16},
number = {2},
pages = {149-158},
keywords = {burden of care, foster carers, looked after children, out-of-home care, support, training},
doi = {https://doi.org/10.1111/j.1365-2206.2010.00722.x},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1365-2206.2010.00722.x},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1365-2206.2010.00722.x},
abstract = {ABSTRACT The challenges of providing sensitive and structured care for children in foster care go well beyond normative experiences of parenting. The present paper describes a mixed-methods study of foster carers' perceived need for support and training, referenced to estimates of their burden of care. Semi-structured interviews were conducted with 17 foster carers in the Canterbury region of New Zealand. Carers' perceived need for support and training were identified through qualitative analysis of interviews using domain analysis method. Carers' burden of care was estimated from the Parenting Stress Index and from a measure of carers' encounters with children's emotional, behavioural and relationship difficulties that was designed for the present study (the Caregiver Behavioural Encounters Index). Foster carers reported substantive, unmet needs for support and training. Foster carers also reported high parenting stress and encounters with a wide range of children's mental health difficulties, including both uncommon and severe difficulties, which together represent an exceptional burden of care. Carers' highest priority need was for training and support on managing and responding to children's mental health difficulties, while their greatest existing support came from Caregiver Liaison Social Workers and other carers. The findings suggest a number of critical implications for practice.},
year = {2011}
}
