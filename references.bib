@article{trocme2004,
author = {Trocm\'{e}, Nico and Knoke, Della and Blackstock, Cindy},
title = {Pathways to the Overrepresentation of {A}boriginal Children in {C}anada’s Child Welfare System},
journal = {Social Service Review},
